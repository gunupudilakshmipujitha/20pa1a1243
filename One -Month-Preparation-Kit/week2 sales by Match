class result{
   public static int sockMerchant(int n, List<Integer> ar) {
        Collections.sort(ar);
        int c=0;
        for(int i=1;i<ar.size();i=i+2){
            if(ar.get(i)==ar.get(i-1)){
                c=c+1;
            }
            else{
                i=i-1;
            }
        }
        return c;
    }
}
