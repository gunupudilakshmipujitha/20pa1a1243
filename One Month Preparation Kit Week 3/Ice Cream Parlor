
    public static List<Integer> icecreamParlor(int m, List<Integer> arr) {
    // Write your code here
        int n=arr.size();
       ArrayList<Integer> a=new ArrayList<Integer>();
       for(int i=0;i<n;i++){
           for(int j=i+1;j<n;j++){
               if(arr.get(i)+arr.get(j)==m){
                   a.add(i+1);
                   a.add(j+1);
               }
           }
       }
       return a;
    }
