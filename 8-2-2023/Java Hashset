
public class Solution {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        String[] p=new String[x];
        String[] q=new String[x];
        for(int i=0;i<x;i++){
            p[i]=sc.next();
            q[i]=sc.next();
        }
        HashSet<String> a=new HashSet<String>();
        for(int i=0;i<x;i++){
            a.add(p[i]+" "+q[i]);
            System.out.println(a.size());
        }
    }
}
